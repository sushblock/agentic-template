build/
dist/
*.egg-info/
.eggs/
*.egg
pip-wheel-metadata/


# Testing
.pytest_cache/
.coverage
.coverage.*
htmlcov/
.tox/
.nox/
.mypy_cache/
.pyre/
.ruff_cache/
.cache/


# Jupyter & notebooks
.ipynb_checkpoints/


# Logs & temp
logs/
.log/
*.log
*.tmp
.tmp/
.temp/
.DS_Store
Thumbs.db


# App data (adjust to your needs)
data/
artifacts/
outputs/
/tmp/


