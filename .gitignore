.vscode/
.idea/
*.iml


# Cursor AI
.cursor/


# JetBrains
*.ipr
*.iws


# OS-specific
.DS_Store
.AppleDouble
.LSOverride
Icon?


# Windows
Desktop.ini
$RECYCLE.BIN/


